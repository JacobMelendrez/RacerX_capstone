    {{#if user}}
    <style>
        .pro_info img{
           float: left; 
           margin-left: 50px;
        }
        .pro_info p{
            margin-left: 200px;
        }
    </style>
    <div class="dashboard">
        <p>Dashboard</p>
    </div>
    <div class="pro_info">
        <img src="/static/baseProfilePic.png"  width="100" height="100" class="profile">
        <p>First Name &nbsp;: {{profile.firstName}}</p>
        <p>Last Name  &nbsp;&nbsp;: {{profile.lastName}}</p>
        <p>User Name  &nbsp;: {{profile.username}}</p>
        <p>email &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: {{profile.email}}</p>
    </div>
    {{else}}
    <style>
        div p{
            font-size: x-large;
            text-align: center;
        }
    </style>
    <div>
        <p> You must sign in before creating an event</p>
    </div>
    {{/if}}
    <script type="text/javascript" src="../static/timezone.js"></script>
    <script type="text/javascript">
        main();
    </script>